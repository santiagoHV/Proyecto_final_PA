<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Registro</servlet-name>
        <servlet-class>controlador.servlet.ServletRegistro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/Registro-completo</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Ingreso</servlet-name>
        <servlet-class>controlador.servlet.ServletIngreso</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Ingreso</servlet-name>
        <url-pattern>/Ingreso</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Pre-registro</servlet-name>
        <servlet-class>controlador.servlet.ServletPreRegistro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Pre-registro</servlet-name>
        <url-pattern>/Pre-registro</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Lector-notas</servlet-name>
        <servlet-class>controlador.servlet.ServletLectorNotas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Lector-notas</servlet-name>
        <url-pattern>/Lector-notas</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Editor-notas</servlet-name>
        <servlet-class>controlador.servlet.ServletPreEditor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Editor-notas</servlet-name>
        <url-pattern>/Editor-notas</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Paso-edicion</servlet-name>
        <servlet-class>controlador.servlet.ServletEditorNotas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Paso-edicion</servlet-name>
        <url-pattern>/Edicion-hecha</url-pattern>
    </servlet-mapping>
</web-app>